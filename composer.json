{
    "name": "hackeresq/laravel-watcher",
    "description": "Watch for specific keys in your Laravel form requests and perform specific functions",
    "keywords": [
        "watch",
        "keys",
        "laravel"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "HackerESQ\\Watcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HackerESQ\\Watcher\\Tests\\": "tests"
        }
    },
    "require": {
        "php" : ">=7.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^6.11"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
